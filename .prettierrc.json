{
    "printWidth": 120,
    "trailingComma": "none",
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "tabWidth": 4,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "plugins": ["prettier-plugin-organize-imports"],
    "vueIndentScriptAndStyle": true
}
